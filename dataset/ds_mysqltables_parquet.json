{
	"name": "ds_mysqltables_parquet",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_adls_landing_mysql",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"schema": {
				"type": "string"
			},
			"table": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().table,'.parquet')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().schema, '/', dataset().table)\n",
					"type": "Expression"
				},
				"fileSystem": "input"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}